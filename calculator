#include <stdio.h>

int main()
{
    int a,b,i;
    float c;
printf("enter two numbers: ");
scanf("%d %d",&a ,&b);
printf("enter case number: ");
scanf("%d",&i);
switch(i)
{
    case 1:
    printf("%d + %d = %f ", a,b,c=a+b);
    break;
    case 2:
    printf("%d * %d = %f ", a,b,c=a*b);
    break;
    case 3:
    if(b==0)
    {
        printf("can't divisor ");
    }
    else
    printf("%d / %d = %f ", a,b,c=a/b);
    break;
    case 4:
    printf("%d - %d = %f ", a,b,c=a-b);
    break;
    default :
    printf("opps");
}
return 0;
}
